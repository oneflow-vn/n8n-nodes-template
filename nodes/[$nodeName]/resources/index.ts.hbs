import { INodeProperties } from 'n8n-workflow'
import runhook from './hooks'

{{#each resources}}
{{importResource this}}
{{/each}}

const resourceSelect: INodeProperties = {{stringify resourceSelect}};

const rawProperties: INodeProperties[] = [
    resourceSelect,
    {{#each resources}}
    {{appendResourceOption this}}
    {{/each}}
];

const { properties } = runhook(rawProperties);

export { properties }
